#include <iostream>
using namespace std;
int main()
{
    int num ;
    int number = 1999;
    while(true)
    {
        cin>>num;
        if(num == number)
        {
            cout<<"Correct"<<endl;
            break;

        }
        else if(num != number)
        {
            cout<<"Wrong"<<endl;
        }
    }
    return 0;
}

