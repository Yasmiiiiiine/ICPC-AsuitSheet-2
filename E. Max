#include <iostream>
using namespace std;
int main()
{
    int num ;
    cin>>num;
    int max = 0;
    for(int i = 1; i<= num; i++)
    {
        int number;
        cin>>number;
        if(i ==1)
        {
            max = number;

        }
        if(number > max)
        {
            max = number;
        }

    }

    cout<<max<<endl;
    return 0;
}
